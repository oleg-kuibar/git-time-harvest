{
  "name": "@weekly-pr-reports/website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 3333",
    "start": "astro dev --port 3333",
    "build": "astro check && astro build",
    "preview": "astro preview --port 3333",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "typecheck": "astro check && tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,mdx,astro,json}\"",
    "clean": "rm -rf dist",
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "test": "echo 'No tests yet'"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.0",
    "@astrojs/check": "^0.3.4",
    "@astrojs/tailwind": "^5.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "astro": "^4.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.300.0",
    "marked": "^11.0.0",
    "million": "^3.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@million/lint": "^1.0.13",
    "@types/marked": "^5.0.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}